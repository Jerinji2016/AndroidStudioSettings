<application>
  <component name="CodeInsightSettings">
    <EXCLUDED_PACKAGE NAME="android.os.Handler" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="JAVA" value="true" />
        <entry key="kotlin" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="CONSOLE:PREVIEW" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="IS_RIGHT_MARGIN_SHOWN" value="false" />
    <option name="ENABLE_RENDERED_DOC" value="true" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value="parameter.hints.old" />
    <settingsMapElement>
      <kotlin.kotlin.lambdas.hints>
        <Settings>
          <option name="implicitReceiversAndParams" value="false" />
        </Settings>
      </kotlin.kotlin.lambdas.hints>
    </settingsMapElement>
  </component>
  <component name="JetEditorOptions">
    <option name="donTShowConversionDialog" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern pattern="\bwidget_off\b.*" />
  </component>
</application>